<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <import type="com.example.menstruacionnavapp.ui.premium.PremiumViewModel" />
        <variable
            name="viewModel"
            type="PremiumViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.premium.PremiumFragment">

        <Button
            android:id="@+id/buttonHome"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Premium Ciclo Fit"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.787" />

        <TextView
            android:id="@+id/text_premium"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:gravity="center"
            android:text="@string/bienvenido_a_ciclofit_premium_n_n_con_ciclofit_premium_podr_s_disfrutar_de_todas_las_funcionalidades_de_la_aplicaci_n_sin_anuncios_n_n_adem_s_tendr_s_acceso_a_funcionalidades_exclusivas_como_estad_sticas_avanzadas_y_entrenamientos_personalizados_de_nuestros_expertos_n_n_realiza_tu_pago_de_suscripci_n_de_forma_f_cil_y_segura_a_trav_s_de_paypal_n_n_desbloquea_todo_tu_potencial_con_ciclofit_premium_en_el_siguiente_bot_n"
            android:textAlignment="center"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.125" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>